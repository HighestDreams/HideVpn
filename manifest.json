{
    "name": "HideVpn",
    "description": "Another free extention vpn to Hide your IP for chrome.",
    "manifest_version": 3,
    "version": "1.0.0",
    "icons": {"128": "/images/icons/128x128.png"},
    "action": {
        "default_popup": "default_popup.html",
        "default_icon": "/images/icons/logo.png"
    },
    "permissions": [
        "storage",
        "activeTab"
    ],
    "host_permissions": [
        "https://www.google.com/*"
    ]
}